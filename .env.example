# Google AI Studio API Key (Gemini - usata da clinical_notes_llm)
GOOGLE_API_KEY=your_google_api_key_here

# AlphaGenome API Key (Google DeepMind)
ALPHAGENOME_API_KEY=your_alphagenome_key_here

# Google API Key (generale)
GOOGLE_GENERAL_API_KEY=your_google_general_key_here

# Google Cloud - Service Account credentials file path
GOOGLE_APPLICATION_CREDENTIALS=path/to/your-service-account.json
